clc
%% 1 primer
format short
m = 2.2 * 10^-2; n = 1/3.1;
otvet_1 = 3*m^2+((2*n^2)^(1/3))/m
%% 2 primer
format shortE
l = 16/21; a = pi / 5;
otvet_2 = (4/3)*(l^3)*(sin(a/2))^2*sqrt(cos(a))
%% 3 primer
a = 3 * 10^-2; b = 0.71; alf = 3/7 * pi;
otvet_3 = sqrt(a*sqrt(b)/(tan(alf))^(1/3))
%% 4 primer
a = sin(5*pi/8); b = - tan(12*pi/180);
format hex
otvet_4 = 3*a^2*sqrt(6.8*(a-b))/(4*(a+b)^3)
format shortE
%% 5 primer 
c = exp(-0.3); alf = 65*pi/180;
otvet_5 = c^3/6*cos(alf/2)*sqrt(sin(alf))
%% 6 primer 
n = exp(3.5); alf = 2*pi/13;
otvet_6 = sqrt(n^3/(16.3*sin(alf)*sin(2*alf)))
%% 7 primer 
s = 18/25; alf = 7*pi/12;
otvet_7 = 5*sin(35*pi/180)*sqrt(s^3*cos(36*pi/180)/(pi^3*tan(alf)))
%% 8 primer 
alf = 2*pi/3; bi = 16*pi/180;
otvet_8 = abs(log10(1+sin(alf)) + log(1-sin(bi)))
%% 9 primer
alf = 12*pi/180; bi = 220*pi/180;
otvet_9 = ((sin(alf+bi))^2-(sin(alf-bi))^2)^(1/3)
%% 10 primer 
format rat
a = 2; b = 2.1649 * 10^-2;
otvet_10 = (log2(b+1.4)/log2(a))^(-3/4)
format shortE
%% 11 primer  
p = 0.013; q = 1.4 * 10^2;
otvet_11 = 3*(p^(-2/3)+q^(-1/2))*(p*q)^(1/3)
%% 12 primer 
% в примере непонятно это степень m или же степень корня поэтому ответа 2
m = 24/37;
otvet_12_1 = (2/3)*m*(m^3*(m^4*m^(1/2))^(1/2))^(1/2)
otvet_12_2 = (2/3)*m*sqrt(m*(m*m^(1/4))^(1/3))
%% 13 primer 
s = 5.403; alf = 28*pi/180;
otvet_13 = 8/3*s*sqrt(s/pi)*(sin(alf/2))^6
%% 14 primer
f = log(7); alf = 1.34*pi/180;
otvet_14 = 2*sqrt(f/pi)*tan(alf)*(sin(alf/2))^2
%% 15 primer
m = log10(13.6); alf = 1.48*pi/180;
otvet_15 = 1/12*m^3*cos(alf)/(sin(alf)+cos(alf))^3
%% 16 primer
h = exp(0.415); alf = 237*pi/180;
format bank
otvet_16 = sqrt(3*h^3)*sin(alf+30*pi/180)*sin(alf-30*pi/180)/(cos(alf))^2
%% 17 primer
format shortE
d = -2.2461*10^-2; alf = 1.146*pi/180;
otvet_17 = alf/3*(log10(d+2)-tan(alf))^2
%% 18 primer
d = exp(2.3); alf = 1;
otvet_18 = d^3*cot(alf)*sqrt((sin(alf))^4-(cos(alf))^4)
%% 19 primer
a = log(1.37); alf = 12*pi/25;
otvet_19 = a^2*sqrt(3)*(sec(alf)+csc(alf))^4/4
%% 20 primer 
a = 0.643; b = pi/7;
otvet_20 = sqrt(pi)/3/(cot(a)+cot(b))^2
%% 21 primer
x = tan(1.21);
otvet_21 = log10(3^(x^2-x-9)+8/27)
%% 22 primer 
a = cos(17*pi/180);
format long
otvet_22 = (5*exp(4*a)*(a+12.36)^2)^(1/5)/log(a+7)
format shortE
%% 23 primer 
x = 2.468*10^-1;
otvet_23 = (log10(x))^2-(27/8)^(x-1)*sin(sqrt(x))
%% 24 primer 
x = 37*pi/180; y = cos(7*pi/24);
otvet_24 = ((x-2)^2*(1/(y-x))^(1/3))^(1/5)
%% 25 primer
a = 8*pi/9; b = pi/10;
otvet_25 = (sin(a)+tan(b))/((a-3*b)^2)^(1/5)
