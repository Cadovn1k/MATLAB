setwd("C:\\Users\\Admin\\Desktop\\Моя прелесть\\TV\\921\\17")
v = read.csv("r2z1.csv")
x = v[[1]]
y = v[[2]]
alpha = 0.05
n = length(x)
u = y - x # Разность выборок
mean_x = sum(x) / n # Среднее значение выборки x
mean_y = sum(y) / n # Среднее значение выборки y
mean_u = sum(u) / n # Среднее значение выборки u
s = sum((u - mean_u)^2) / n # Смещенная дисперсия выборок
ndf = n - 1 # Число степеней свободы
Ccr = qt(1 - alpha, ndf) # С-критическая константа
T = mean_u * sqrt(n - 1) / sqrt(s) # Статистика одновыборочного критерия Стьюдента
p = 1 - pt(T, ndf) # p-значение
